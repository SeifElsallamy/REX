{
  "manifest_version": 3,
  "name": "REX",
  "version": "1.0",
  "description": "Apply and Save Regex to Web Page and its Scripts",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
	"tabs"
	
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
